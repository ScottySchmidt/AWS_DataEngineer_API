~ $ STACK=$(aws cloudformation list-stacks --stack-status-filter CREATE_COMPLETE UPDATE_COMPLETE --query 'reverse(sort_by(StackSummaries,&CreationTime))[0].StackName' --output text)

~ $ 
~ $ echo STACK=$STACK
STACK=BlsPipelineStack
~ $ 
~ $ aws cloudformation describe-stacks --stack-name "$STACK" --query 'Stacks[0].StackStatus' --output text --no-cli-pager
UPDATE_COMPLETE
~ $ 
